<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#734d26" />
            <gradient android:angle="-90" android:startColor="#734d26" android:endColor="#734d26"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#734d26" />
            <solid android:color="#734d26"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#734d26" />
            <gradient android:angle="-90" android:startColor="#d9b38c" android:endColor="#734d26" />
        </shape>
    </item>
</selector>
